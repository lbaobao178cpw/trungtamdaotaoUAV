{
  "name": "trung-tam-uav-monorepo",
  "version": "1.0.0",
  "scripts": {
    "install:all": "npm install && npm install --prefix frontend && npm install --prefix frontend-admin && npm install --prefix backend",
    "dev:client": "npm run dev --prefix frontend",
    "dev:admin": "npm run dev --prefix frontend-admin",
    "dev:backend": "npm run dev --prefix backend",
    "dev": "concurrently --names \"CLIENT,ADMIN,API\" --prefix-colors \"blue,magenta,green\" \"npm run dev:client\" \"npm run dev:admin\" \"npm run dev:backend\"",
    "build:client": "npm run build --prefix frontend",
    "build:admin": "npm run build --prefix frontend-admin",
    "build": "npm run build:client && npm run build:admin",
    "preview:client": "npm run preview --prefix frontend",
    "preview:admin": "npm run preview --prefix frontend-admin"
  },
  "dependencies": {
    "cloudinary": "^2.9.0",
    "dotenv": "^17.2.3",
    "fs-extra": "^11.3.3",
    "lucide-react": "^0.554.0",
    "multer": "^2.0.2",
    "react-activation": "^0.13.4",
    "react-toastify": "^11.0.5",
    "sharp": "^0.34.5",
    "sonner": "^2.0.7",
    "sweetalert2": "^11.26.17"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
